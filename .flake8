[flake8]
max-line-length = 88
# Reglas que entran en conflicto con black
extend-ignore = E203, W503 
exclude =
    .git,
    __pycache__,
    migrations,
    .venv,
    env,
    .env,
    .mypy_cache,
    .pytest_cache,
    .tox,
    venv,
    media,
    static,
    node_modules,
    *.min.js,
    *.min.css,
    settings.py 

format = black
max-complexity = 10